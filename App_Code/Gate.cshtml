@functions {
    public static bool Priv(string url)
    {
        string yflb = Session["yflb"] == null ? "" : Session["yflb"].ToString();
        var priv = w_test.Models.BaseData.privs.Find(p => p.yflb == yflb && p.url == url);
        // 找到就说明有权限
        if (priv == null)
        {
            return false;
        }
        return true;
    }
}